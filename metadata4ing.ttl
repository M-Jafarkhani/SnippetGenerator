               rdfs:subPropertyOf obo:BFO_0000063 ;
               rdfs:domain obo:BFO_0000015 ;
               rdfs:range obo:BFO_0000015 ;
               skos:definition "X immediately precedes_Y iff: end(X) simultaneous_with start(Y)" ;
               skos:editorialNote "changed by m4i: added skos:prefLabel"@en ;
               skos:prefLabel "immediately precedes"@en ,
                              "ist unmittelbare Voraussetzung für Schritt"@de .


###  http://purl.obolibrary.org/obo/RO_0002224
obo:RO_0002224 rdf:type owl:ObjectProperty ;
               rdfs:subPropertyOf obo:BFO_0000051 ;
               rdf:type owl:TransitiveProperty ;
               rdfs:domain m4i:ProcessingStep ;
               rdfs:range m4i:ProcessingStep ;
               rdfs:seeAlso <http://ontologydesignpatterns.org/wiki/Submissions:Componency> ;
               skos:altLabel "started by"@en ;
               skos:definition "x starts with y if and only if x has part y and the time point at which x starts is equivalent to the time point at which y starts. Formally: α(y) = α(x) ∧ ω(y) < ω(x), where α is a function that maps a process to a start point, and ω is a function that maps a process to an end point."@en ;
               skos:editorialNote "changed by m4i: added german labels, added skos:prefLabel"@en ;
               skos:prefLabel "beginnt mit"@de ,
                              "starts with"@en .


###  http://purl.obolibrary.org/obo/RO_0002230
obo:RO_0002230 rdf:type owl:ObjectProperty ;
               rdfs:subPropertyOf obo:BFO_0000051 ;
               rdf:type owl:TransitiveProperty ;
               rdfs:domain m4i:ProcessingStep ;
               rdfs:range m4i:ProcessingStep ;
               skos:altLabel "finished by"@en ;
               skos:definition "x ends with y if and only if x has part y and the time point at which x ends is equivalent to the time point at which y ends. Formally: α(y) > α(x) ∧ ω(y) = ω(x), where α is a function that maps a process to a start point, and ω is a function that maps a process to an end point."@en ;
               skos:editorialNote "changed by m4i: added german labels, added skos:prefLabel"@en ;
               skos:prefLabel "endet mit"@de ,
                              "ends with"@en .


