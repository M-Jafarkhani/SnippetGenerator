###  http://www.molmod.info/semantics/pims-ii.ttl#isAdmissibleUnitFor
pims-ii:isAdmissibleUnitFor rdf:type owl:ObjectProperty ;
                            rdfs:comment "redundant information: admissible unit can be deduced from quantity kind. If one wants to enforce the use of a specific unit, use a SHACL contraint."^^xsd:string ;
                            owl:deprecated "true"^^xsd:boolean ;
                            skos:definition "DEPRECATED (x isAdmissibleUnitFor y) means that x is a unit that can be associated with the variable y; subsumption under msHPIPIR is explained as follows: There is a sign z for the variable y (where z is a description of admissible values of y) of which x is constitutive (and with which x hence overlaps)"@en ;
                            rdfs:domain qudt:Unit ;
                            rdfs:range [ rdf:type owl:Class ;
                                         owl:unionOf ( qudt:QuantityKind
                                                       m4i:NumericalVariable
                                                     )
                                       ] ;
                            skos:editorialNote "changed by m4i: added skos:prefLabel, adjusted german labels, harmonized spelling to OBO naming conventions (lower case, space separated)" ;
                            skos:prefLabel "is admissible unit for"@en ,
                                           "ist zulässige Einheit für"@de .


###  http://www.molmod.info/semantics/pims-ii.ttl#isAdmissibleValueFor
pims-ii:isAdmissibleValueFor rdf:type owl:ObjectProperty ;
                             rdfs:domain pims-ii:Value ;
                             rdfs:range pims-ii:Variable ;
                             skos:definition "(x isAdmissibleValueFor y) means that x is a value that can be assigned to the variable y; subsumption under msIPIRIR is explained as follows: There is a sign z for the variable y (where z is the range of admissible values of y), for which in turn there is another sign z' (where z' lists members of z, explicitly including x) of which x is a part"@en ;
                             skos:editorialNote "changed by m4i: added skos:prefLabel, adjusted german labels, harmonized spelling to OBO naming conventions (lower case, space separated)" ;
                             skos:prefLabel "is admissible value for"@en ,
                                            "ist zulässiger Wert für"@de .


###  http://www.molmod.info/semantics/pims-ii.ttl#isAssignmentFor
pims-ii:isAssignmentFor rdf:type owl:ObjectProperty ;
                        rdfs:domain pims-ii:Assignment ;
                        skos:broader pims-ii:isRepresentamenFor ;
                        skos:definition "(o isAssignment for d) means that d: x = y is an assignment that refers to o; e.g., x may be temperature, y may be a temperature value, and o may be an object of which the temperature has been measured or some other context to which the assignment refers"@en ;
                        skos:editorialNote "changed by m4i: added skos:prefLabel, adjusted german labels, harmonized spelling to OBO naming conventions (lower case, space separated)" ;
                        skos:prefLabel "is assignment for"@en ,
                                       "ist Zuweisung in Bezug auf"@de .


###  http://www.molmod.info/semantics/pims-ii.ttl#isValueInAssignment
pims-ii:isValueInAssignment rdf:type owl:ObjectProperty ;
                            rdfs:domain pims-ii:Value ;
                            rdfs:range pims-ii:Assignment ;
                            skos:broader pims-ii:isSemioticallyConstitutiveOf ;
                            skos:definition "(y isValueInAssignment a) means that a: x = y is an assignment where the value y is assigned to a variable x"@en ;
                            skos:editorialNote "changed by m4i: added skos:prefLabel, adjusted german labels, harmonized spelling to OBO naming conventions (lower case, space separated)" ;
                            skos:prefLabel "is value in assignment"@en ,
                                           "ist Wert in Zuweisung"@de .


###  http://www.molmod.info/semantics/pims-ii.ttl#isVariableInAssignment
pims-ii:isVariableInAssignment rdf:type owl:ObjectProperty ;
                               rdfs:domain pims-ii:Variable ;
                               rdfs:range pims-ii:Assignment ;
                               skos:broader pims-ii:isSemioticallyConstitutiveOf ;
                               skos:definition "(x isVariableInAssignment a) means that a: x = y is an assignment where a value y is assigned to the variable x"@en ;
                               skos:editorialNote "changed by m4i: added skos:prefLabel, adjusted german labels, harmonized spelling to OBO naming conventions (lower case, space separated)" ;
                               skos:prefLabel "is variable in assignment"@en ,
                                              "ist Variable in Zuweisung"@de .

