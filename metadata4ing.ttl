###  http://w3id.org/nfdi4ing/metadata4ing#Method
m4i:Method rdf:type owl:Class ;
           rdfs:subClassOf obo:BFO_0000017 ;
           skos:definition "Abstract description of a method for analysis, generation and transformation of data and material objects like, e.g., 'Fourier Transform Infrared Spectroscopy' or 'Molecular Dynamics Simulation'. Note that while values for relevant parameters should be provided, m4i:Method does not refer to the actual execution of the process that is described (use m4i:ProcessingStep for that), but rather a description of the underlying principle."@en ;
           skos:prefLabel "Methode"@de ,
                          "method"@en .


###  http://w3id.org/nfdi4ing/metadata4ing#NumericalAssignment
m4i:NumericalAssignment rdf:type owl:Class ;
                        rdfs:subClassOf pims-ii:Assignment ,
                                        [ rdf:type owl:Restriction ;
                                          owl:onProperty [ owl:inverseOf pims-ii:isValueInAssignment
                                                         ] ;
                                          owl:someValuesFrom si:Real
                                        ] ,
                                        [ rdf:type owl:Restriction ;
                                          owl:onProperty [ owl:inverseOf pims-ii:isVariableInAssignment
                                                         ] ;
                                          owl:minCardinality "1"^^xsd:nonNegativeInteger
                                        ] ;
                        skos:definition "Assignment by which a variable (e.g., a physical property) is assigned an si:Real quantity value (scalar x measurement unit) in some context (e.g., applied to some object of research)"@en ;
                        skos:prefLabel "numerical assignment"@en ,
                                       "numerische Zuweisung"@de .


###  http://w3id.org/nfdi4ing/metadata4ing#NumericalVariable
m4i:NumericalVariable rdf:type owl:Class ;
                      rdfs:subClassOf pims-ii:Variable ;
                      skos:closeMatch qudt:Quantity ,
                                      si:Real ;
                      skos:definition "A variable that expects a float or integer as a value"@en ,
                                      "Eine Variable, die eine Zahl als Wert erwartet"@de ;
                      skos:prefLabel "numerical variable"@en ,
                                     "numerische Variable"@de .


