@prefix m4i: <http://w3id.org/nfdi4ing/metadata4ing#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix prov: <http://www.w3.org/ns/prov#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

m4i:ProjectMember a owl:NamedIndividual,
        prov:Role ;
    rdfs:seeAlso "https://datacite-metadata-schema.readthedocs.io/en/4.5/appendices/appendix-1/contributorType/#projectmember"^^xsd:anyURI ;
    skos:definition "Person on the membership list of a designated project/project team"@en ;
    skos:prefLabel "Projektmitglied"@de,
        "project member"@en ;
    skos:scopeNote "This vocabulary may or may not indicate the quality, quantity, or substance of the person's involvement."@en .

