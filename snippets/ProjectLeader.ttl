@prefix m4i: <http://w3id.org/nfdi4ing/metadata4ing#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix prov: <http://www.w3.org/ns/prov#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

m4i:ProjectLeader a owl:NamedIndividual,
        prov:Role ;
    rdfs:seeAlso "https://datacite-metadata-schema.readthedocs.io/en/4.5/appendices/appendix-1/contributorType/#projectleader"^^xsd:anyURI ;
    skos:definition "Person officially designated as head of project team or sub-project team instrumental in the work necessary to development of the resource"@en ;
    skos:prefLabel "Projektleiter*in"@de,
        "project leader"@en ;
    skos:scopeNote "The Project Leader is not \"removed\" from the work that resulted in the resource; he or she remains intimately involved"@en .

